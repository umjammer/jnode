<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<Configuration status="info">
  <Appenders>
    <Console name="Console" target="SYSTEM_ERR">
      <PatternLayout pattern="%d{ISO8601} [%p] %m%n\tat %t%x %c.%M(%F:%L)\n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
    <Logger name="org.jnode.partitions" level="debug" additivity="false">
        <AppenderRef ref="Console"/>
    </Logger>
  </Loggers>
</Configuration>